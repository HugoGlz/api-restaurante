<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mx.restaurante.beans.DetalleModificador" table="tbl_det_mod">
        <id name="id" type="java.lang.Integer">
            <column name="id_det_mod" />
            <generator class="identity" />
        </id>
        <property name="detalle" type="java.lang.Integer">
            <column name="id_det"/>
        </property>
        <property name="modificador" type="java.lang.Integer">
            <column name="id_modificador"/>
        </property>
        <property name="descripcion" type="java.lang.String">
            <column name="descripcion"/>
        </property>
        <property name="precio" type="java.lang.Double">
            <column name="precio"/>
        </property>
        <property name="estatus" type="java.lang.Integer">
            <column name="estatus"/>
        </property>
        <property name="prefijo" type="java.lang.Integer">
            <column name="prefijo"/>
        </property>
    </class>
</hibernate-mapping>
