<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mx.restaurante.beans.Operacion" table="tbl_operaciones">
        <id name="id" type="java.lang.Integer">
            <column name="id_oper" />
            <generator class="identity" />
        </id>        
        <property name="fecha" type="java.util.Calendar">
            <column name="fecha" />
        </property>
        <property name="estatus" type="java.lang.Integer">
            <column name="estatus"/>
        </property>
        <property name="usrInicia" type="java.lang.Integer">
            <column name="usr_inicia"/>
        </property>
        
        <property name="cajero" type="java.lang.Integer">
            <column name="id_cajero"/>
        </property>
        
        <property name="turno" type="java.lang.Integer">
            <column name="turno"/>
        </property>
        
        <property name="fondo" type="java.lang.Double">
            <column name="fondo"/>
        </property>
	</class>
</hibernate-mapping>
