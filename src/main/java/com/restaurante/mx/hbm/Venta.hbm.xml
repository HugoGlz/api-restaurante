<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mx.restaurante.beans.Venta" table="tbl_enc_vtas">
        <id name="id" type="java.lang.Integer">
            <column name="id_enc" />
            <generator class="identity" />
        </id>
        <property name="fecha" type="java.util.Calendar">
            <column name="fecha"/>
        </property>        
        <property name="tipo" type="java.lang.Integer">
            <column name="tipo"/>
        </property>
        <property name="tipoNota" type="java.lang.Integer">
            <column name="tipo_nota"/>
        </property>
        <property name="turno" type="java.lang.Integer">
            <column name="turno"/>
        </property>
        <property name="mesa" type="java.lang.Integer">
            <column name="num_mesa"/>
        </property>
        <property name="mesero" type="java.lang.Integer">
            <column name="id_mesero"/>
        </property>
        <property name="pax" type="java.lang.Integer">
            <column name="pax"/>
        </property>
        <property name="abre" type="java.util.Calendar">
            <column name="abre"/>
        </property>
        <property name="descuento" type="java.lang.Double">
            <column name="descuento"/>
        </property>
        <property name="usuarioDescuento" type="java.lang.Integer">
            <column name="usr_descuento"/>
        </property>
        <property name="usuarioCancelacion" type="java.lang.Integer">
            <column name="usr_cancelacion"/>
        </property>
        <property name="horaCancelacion" type="java.util.Calendar">
            <column name="hora_cancelacion"/>
        </property>
        <property name="horaLlegada" type="java.util.Calendar">
            <column name="hora_llegada"/>
        </property>
        <property name="estatus" type="java.lang.Integer">
            <column name="estatus"/>
        </property>
    </class>
</hibernate-mapping>
